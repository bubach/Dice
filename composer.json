{
    "name": "garrettw/dice",
    "description": "My version of Dice, a minimalist Dependency Injection Container for PHP. Fully compatible with upstream. My additions: PSR-4 compatibility, Rule class constructor can set properties, beautified code.",
    "license": "BSD-2-Clause",
    "homepage": "http://r.je/dice.html",
    "keywords": ["Dependency injection", "DI", "Dependency injection container", "DIC", "ioc"],
    "minimum-stability": "dev",
    "authors": [
    	{
    		"name": "Tom Butler",
    		"email": "tom@r.je"
    	},
    	{
    	    "name": "Garrett Whitehorn",
    	    "homepage": "http://garrettw.net/"
    	}
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phpdocumentor/phpdocumentor": "~2.8",
        "phpspec/phpspec": "~2.1",
        "fabpot/php-cs-fixer": "~1.8"
    },
    "autoload": {
        "psr-4": {
            "Dice\\": "src/"
        }
    }
}
